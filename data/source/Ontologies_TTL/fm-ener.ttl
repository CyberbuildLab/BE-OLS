@prefix : <https://w3id.org/sml/2/owl/def#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sml: <https://w3id.org/sml/def#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix sml-term: <https://w3id.org/sml/term#> .
@prefix quantitykind: <http://qudt.org/vocab/quantitykind/> .
@base <https://w3id.org/sml/2/owl/def> .

<https://w3id.org/sml/2/owl/def> rdf:type owl:Ontology ;
                                  owl:imports <http://qudt.org/2.1/schema/qudt> ,
                                              <http://qudt.org/2.1/vocab/quantitykind> ,
                                              <http://qudt.org/2.1/vocab/unit> ,
                                              <http://www.opengis.net/ont/geosparql> ,
                                              <http://www.w3.org/2004/02/skos/core> ,
                                              <http://www.w3.org/2006/time> ,
                                              <http://www.w3.org/ns/shacl> ,
                                              sosa: ,
                                              <https://w3id.org/sml/2/rdfs/def> ,
                                              <https://w3id.org/sml/2/skos/term> ,
                                              <https://w3id.org/sml/owl/def> ,
                                              <https://w3id.org/sml/rdfs/def> ,
                                              <https://w3id.org/sml/shacl/def> ,
                                              <https://w3id.org/sml/skos/term> .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2003/01/geo/wgs84_pos#alt
<http://www.w3.org/2003/01/geo/wgs84_pos#alt> rdfs:comment """The WGS84 altitude of a SpatialThing (decimal meters 
above the local reference ellipsoid).""" ;
                                              rdfs:label "altitude" ;
                                              rdfs:domain <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> .


###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
<http://www.w3.org/2003/01/geo/wgs84_pos#lat> rdfs:comment "The WGS84 latitude of a SpatialThing (decimal degrees)." ;
                                              rdfs:label "latitude" ;
                                              rdfs:domain <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> .


###  http://www.w3.org/2003/01/geo/wgs84_pos#location
<http://www.w3.org/2003/01/geo/wgs84_pos#location> rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/based_near> .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
<http://www.w3.org/2003/01/geo/wgs84_pos#long> rdfs:comment "The WGS84 longitude of a SpatialThing (decimal degrees)." ;
                                               rdfs:label "longitude" ;
                                               rdfs:domain <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> .


###  https://w3id.org/sml/def#abbreviation
sml:abbreviation rdf:type owl:AnnotationProperty ;
                 rdfs:subPropertyOf skos:altLabel ;
                 rdfs:range xsd:string ;
                 rdfs:domain sml:TopConcept .


#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/2003/01/geo/wgs84_pos#location
<http://www.w3.org/2003/01/geo/wgs84_pos#location> rdfs:range <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> .


###  https://w3id.org/sml/def#aggregationStateType
sml:aggregationStateType rdf:type owl:ObjectProperty ;
                         rdfs:domain sml:Matter ;
                         rdfs:range sml:AggregationStateType .


###  https://w3id.org/sml/def#begins
sml:begins rdf:type owl:ObjectProperty ;
           rdfs:domain sml:Event ;
           rdfs:range sml:State .


###  https://w3id.org/sml/def#chemicalFormula
sml:chemicalFormula rdfs:domain sml:Matter ;
                    rdfs:range xsd:string .


###  https://w3id.org/sml/def#connectsObject
sml:connectsObject rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( sml:Connection
                                               sml:Interface
                                             )
                               ] ;
                   rdfs:range sml:PhysicalObject .


###  https://w3id.org/sml/def#connectsPort
sml:connectsPort rdf:type owl:ObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( sml:Connection
                                             sml:Interface
                                           )
                             ] ;
                 rdfs:range sml:Port .


###  https://w3id.org/sml/def#consistsOf
sml:consistsOf rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( sml:Mixture
                                           sml:RealObject
                                         )
                           ] ;
               rdfs:range [ rdf:type owl:Class ;
                            owl:unionOf ( sml:Matter
                                          sml:MatterPortion
                                        )
                          ] .


###  https://w3id.org/sml/def#contains
sml:contains rdf:type owl:ObjectProperty ;
             rdfs:domain sml:SpatialRegion ;
             rdfs:range sml:RealObject .


###  https://w3id.org/sml/def#ends
sml:ends rdf:type owl:ObjectProperty ;
         rdfs:domain sml:Event ;
         rdfs:range sml:State .


###  https://w3id.org/sml/def#executes
sml:executes rdf:type owl:ObjectProperty ;
             rdfs:domain sml:PhysicalObject ;
             rdfs:range sml:Activity .


###  https://w3id.org/sml/def#hasBoundary
sml:hasBoundary rdf:type owl:ObjectProperty ;
                rdfs:domain sml:ConcreteConcept ;
                rdfs:range sml:GeometricEntity .


###  https://w3id.org/sml/def#hasFunctionalPart
sml:hasFunctionalPart rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf sml:hasPart ;
                      rdfs:domain sml:FunctionalEntity ;
                      rdfs:range sml:FunctionalEntity .


###  https://w3id.org/sml/def#hasInterior
sml:hasInterior rdf:type owl:ObjectProperty ;
                rdfs:domain sml:ConcreteConcept ;
                rdfs:range sml:GeometricEntity .


###  https://w3id.org/sml/def#hasPart
sml:hasPart rdf:type owl:ObjectProperty ;
            rdfs:domain sml:ConcreteConcept ;
            rdfs:range sml:ConcreteConcept .


###  https://w3id.org/sml/def#hasPartsGroup
sml:hasPartsGroup rdf:type owl:ObjectProperty ;
                  rdfs:domain sml:ConcreteConcept ;
                  rdfs:range rdfs:Container .


###  https://w3id.org/sml/def#hasQuantityKind
sml:hasQuantityKind rdf:type owl:ObjectProperty ;
                    rdfs:range qudt:QuantityKind .


###  https://w3id.org/sml/def#hasRequirement
sml:hasRequirement rdf:type owl:ObjectProperty ;
                   rdfs:domain sml:TopConcept ;
                   rdfs:range sml:Requirement .


###  https://w3id.org/sml/def#hasSolution
sml:hasSolution rdf:type owl:ObjectProperty ;
                rdfs:domain sml:FunctionalEntity ;
                rdfs:range sml:TechnicalEntity .


###  https://w3id.org/sml/def#hasState
sml:hasState rdf:type owl:ObjectProperty ;
             rdfs:domain sml:Entity ;
             rdfs:range sml:State .


###  https://w3id.org/sml/def#hasTechnicalPart
sml:hasTechnicalPart rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf sml:hasPart ;
                     rdfs:domain sml:TechnicalEntity ;
                     rdfs:range sml:TechnicalEntity .


###  https://w3id.org/sml/def#hasTemporalEntity
sml:hasTemporalEntity rdf:type owl:ObjectProperty ;
                      rdfs:domain sml:ConcreteConcept ;
                      rdfs:range time:TemporalEntity .


###  https://w3id.org/sml/def#hasUnit
sml:hasUnit rdf:type owl:ObjectProperty ;
            rdfs:range qudt:Unit .


###  https://w3id.org/sml/def#isBoundBy
sml:isBoundBy rdf:type owl:ObjectProperty ;
              rdfs:domain sml:SpatialRegion ;
              rdfs:range sml:PhysicalObject .


###  https://w3id.org/sml/def#isConnectedTo
sml:isConnectedTo rdf:type owl:ObjectProperty ;
                  rdfs:domain sml:PhysicalObject ;
                  rdfs:range sml:PhysicalObject .


###  https://w3id.org/sml/def#isDescribedBy
sml:isDescribedBy rdf:type owl:ObjectProperty ;
                  rdfs:domain sml:TopConcept ;
                  rdfs:range sml:InformationObject .


###  https://w3id.org/sml/def#isImplementedBy
sml:isImplementedBy rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf sml:hasSolution ;
                    rdfs:domain sml:FunctionalEntity ;
                    rdfs:range sml:TechnicalEntity ;
                    rdfs:seeAlso sml-term:isImplementedBy ;
                    skos:definition "The technical entity that implements a functional entity"@en ;
                    skos:prefLabel "is implemented by"@en .


###  https://w3id.org/sml/def#isPlanFor
sml:isPlanFor rdf:type owl:ObjectProperty ;
              rdfs:domain sml:PlannedEntity ;
              rdfs:range sml:RealizedEntity .


###  https://w3id.org/sml/def#isRealizedBy
sml:isRealizedBy rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf sml:isPlanFor ;
                 rdfs:domain sml:PlannedEntity ;
                 rdfs:range sml:RealizedEntity ;
                 rdfs:seeAlso sml-term:isRealizedBy ;
                 skos:definition "The realized entity that realizes a planned entity"@en ;
                 skos:prefLabel "is realized by"@en .


###  https://w3id.org/sml/def#overConnection
sml:overConnection rdf:type owl:ObjectProperty ;
                   rdfs:domain sml:Interaction ;
                   rdfs:range sml:Connection .


###  https://w3id.org/sml/def#overInterface
sml:overInterface rdf:type owl:ObjectProperty ;
                  rdfs:domain sml:Interaction ;
                  rdfs:range sml:Interface .


###  https://w3id.org/sml/def#participatesIn
sml:participatesIn rdf:type owl:ObjectProperty ;
                   rdfs:domain sml:PhysicalObject ;
                   rdfs:range sml:Interaction .


###  https://w3id.org/sml/def#portion
sml:portion rdf:type owl:ObjectProperty ;
            rdfs:domain sml:MatterPortion ;
            rdfs:range sml:QuantityValue .


###  https://w3id.org/sml/def#requirementSeverityType
sml:requirementSeverityType rdf:type owl:ObjectProperty ;
                            rdfs:domain sml:Requirement ;
                            rdfs:range sml:RequirementSeverityType .


###  https://w3id.org/sml/def#requirementSourceType
sml:requirementSourceType rdf:type owl:ObjectProperty ;
                          rdfs:domain sml:Requirement ;
                          rdfs:range sml:RequirementSourceType .


###  https://w3id.org/sml/def#requirementTopicType
sml:requirementTopicType rdf:type owl:ObjectProperty ;
                         rdfs:domain sml:Requirement ;
                         rdfs:range sml:RequirementTopicType .


###  https://w3id.org/sml/def#transferType
sml:transferType rdf:type owl:ObjectProperty ;
                 rdfs:domain sml:Interaction ;
                 rdfs:range sml:TransferType .


###  https://w3id.org/sml/def#transforms
sml:transforms rdf:type owl:ObjectProperty ;
               rdfs:domain sml:Activity ;
               rdfs:range sml:Object .


###  https://w3id.org/sml/def#triggers
sml:triggers rdf:type owl:ObjectProperty ;
             rdfs:domain sml:Entity ;
             rdfs:range sml:Event .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/sml/def#chemicalFormula
sml:chemicalFormula rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/2003/01/geo/wgs84_pos#Point
<http://www.w3.org/2003/01/geo/wgs84_pos#Point> rdf:type owl:Class ;
                                                rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ;
                                                rdfs:comment """ 
Uniquely identified by lat/long/alt. i.e.

spaciallyIntersects(P1, P2) :- lat(P1, LAT), long(P1, LONG), alt(P1, ALT),
  lat(P2, LAT), long(P2, LONG), alt(P2, ALT).

sameThing(P1, P2) :- type(P1, Point), type(P2, Point), spaciallyIntersects(P1, P2).
  """ ,
                                                             """A point, typically described using a coordinate system relative to Earth, such as WGS84.
  """ ;
                                                rdfs:label "point" .


###  http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing
<http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> rdf:type owl:Class ;
                                                       rdfs:comment """Anything with spatial extent, i.e. size, shape, or position.
 e.g. people, places, bowling balls, as well as abstract areas like cubes.
""" ;
                                                       rdfs:label "SpatialThing" .


###  http://www.w3.org/2006/time#TemporalEntity
time:TemporalEntity rdfs:subClassOf sml:Representation .


###  https://w3id.org/sml/def#AbstractConcept
sml:AbstractConcept rdf:type owl:Class ;
                    rdfs:subClassOf sml:TopConcept ;
                    rdfs:seeAlso sml-term:AbstractConcept ;
                    skos:definition "Concept that forms a manifestation and demarcation in an abstract space. An abstract concept has no life cycle"@en ;
                    skos:prefLabel "Abstract concept"@en .


###  https://w3id.org/sml/def#Activity
sml:Activity rdf:type owl:Class ;
             rdfs:subClassOf sml:Entity ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty sml:hasPart ;
                               owl:allValuesFrom sml:Activity
                             ] .


###  https://w3id.org/sml/def#AggregationStateType
sml:AggregationStateType rdf:type owl:Class ;
                         owl:equivalentClass [ rdf:type owl:Class ;
                                               owl:oneOf ( sml:Gas
                                                           sml:Liquid
                                                           sml:Plasma
                                                           sml:Solid
                                                         )
                                             ] .


###  https://w3id.org/sml/def#AmountOfBulkMatter
sml:AmountOfBulkMatter rdf:type owl:Class ;
                       rdfs:subClassOf sml:RealObject ;
                       rdfs:seeAlso sml-term:AmountOfBulkMatter ;
                       skos:definition "A real object that consists of a continuous amount of non-rigid matter, held together primarily by external forces (gravity or confinement)"@en ;
                       skos:prefLabel "Amount of bulk matter"@en .


###  https://w3id.org/sml/def#ChemicalCompound
sml:ChemicalCompound rdf:type owl:Class ;
                     rdfs:subClassOf sml:PureSubstance ;
                     rdfs:seeAlso sml-term:ChemicalBonding ;
                     skos:definition "Pure substance consisting of two or more chemical elements that have a chemical bond with each other. In a chemical compound, the elements occur in a fixed ratio. A compound can be broken down into simpler substances by means of chemical reactions"@en ;
                     skos:prefLabel "Chemical compound"@en .


###  https://w3id.org/sml/def#ChemicalElement
sml:ChemicalElement rdf:type owl:Class ;
                    rdfs:subClassOf sml:PureSubstance ;
                    rdfs:seeAlso sml-term:ChemicalElement ;
                    skos:definition "Pure substance that is made up of atoms with the same atomic number. A chemical element cannot be decomposed through chemical reactions"@en ;
                    skos:prefLabel "Chemical element"@en .


###  https://w3id.org/sml/def#ConceptType
sml:ConceptType rdf:type owl:Class ;
                rdfs:subClassOf sml:Type ;
                rdfs:seeAlso sml-term:ConceptType ;
                skos:definition "A meta-concept having concepts as instances that can have attributes or relations"@en ;
                skos:prefLabel "Concept type"@en .


###  https://w3id.org/sml/def#ConcreteConcept
sml:ConcreteConcept rdf:type owl:Class ;
                    rdfs:subClassOf sml:TopConcept .


###  https://w3id.org/sml/def#Connection
sml:Connection rdf:type owl:Class ;
               rdfs:subClassOf sml:PhysicalObject .


###  https://w3id.org/sml/def#DiscreteObject
sml:DiscreteObject rdf:type owl:Class ;
                   rdfs:subClassOf sml:RealObject ;
                   rdfs:seeAlso sml-term:DiscreteObject ;
                   skos:definition "A real object consisting of a contiguous amount of form-retaining matter, held together primarily by internal forces (gravity or electromagnetic force)"@en ;
                   skos:prefLabel "Discrete object"@en .


###  https://w3id.org/sml/def#Entity
sml:Entity rdf:type owl:Class ;
           rdfs:subClassOf sml:ConcreteConcept .


###  https://w3id.org/sml/def#EnumerationType
sml:EnumerationType rdf:type owl:Class ;
                    rdfs:subClassOf rdfs:Container ,
                                    sml:Type ;
                    rdfs:seeAlso sml-term:EnumerationType ;
                    skos:definition "A meta-concept having concepts as instances having no further attributes or relations (annotations only)"@en ;
                    skos:prefLabel "Enumeration type"@en .


###  https://w3id.org/sml/def#Event
sml:Event rdf:type owl:Class ;
          rdfs:subClassOf sml:ConcreteConcept ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sml:hasPart ;
                            owl:allValuesFrom sml:Event
                          ] .


###  https://w3id.org/sml/def#Function
sml:Function rdf:type owl:Class ;
             rdfs:subClassOf sml:Activity ,
                             sml:FunctionalActivity ;
             skos:definition "An activity performed by an object whose external behavior contributes to goals set by one or more technical entities"@en ;
             skos:prefLabel "Function"@en .


###  https://w3id.org/sml/def#FunctionalEntity
sml:FunctionalEntity rdf:type owl:Class ;
                     rdfs:subClassOf sml:Entity ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty sml:hasPart ;
                                       owl:allValuesFrom sml:Entity
                                     ] ;
                     owl:disjointWith sml:TechnicalEntity .


###  https://w3id.org/sml/def#GeometricEntity
sml:GeometricEntity rdf:type owl:Class ;
                    rdfs:subClassOf sml:Representation .


###  https://w3id.org/sml/def#HeterogeneousMixture
sml:HeterogeneousMixture rdf:type owl:Class ;
                         rdfs:subClassOf sml:Mixture ;
                         rdfs:seeAlso sml-term:HeterogeneousMixture ;
                         skos:definition "A mixture that is separated into different regions or phases that have different compositions or properties"@en ;
                         skos:prefLabel "Heterogeneous mixture"@en .


###  https://w3id.org/sml/def#HomogeneousMixture
sml:HomogeneousMixture rdf:type owl:Class ;
                       rdfs:subClassOf sml:Mixture ;
                       rdfs:seeAlso sml-term:HomogeneousMixture ;
                       skos:definition "A mixture that is uniform throughout, and has only one phase"@en ;
                       skos:prefLabel "Homogeneous mixture"@en .


###  https://w3id.org/sml/def#InformationObject
sml:InformationObject rdf:type owl:Class ;
                      rdfs:subClassOf sml:Object .


###  https://w3id.org/sml/def#Interaction
sml:Interaction rdf:type owl:Class ;
                rdfs:subClassOf sml:Activity .


###  https://w3id.org/sml/def#Interface
sml:Interface rdf:type owl:Class ;
              rdfs:subClassOf sml:SpatialRegion .


###  https://w3id.org/sml/def#Matter
sml:Matter rdf:type owl:Class ;
           rdfs:subClassOf sml:PhysicalObject .


###  https://w3id.org/sml/def#MatterPortion
sml:MatterPortion rdf:type owl:Class ;
                  rdfs:subClassOf sml:RelationReference ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty rdf:value ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass sml:Matter
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty sml:portion ;
                                    owl:cardinality "1"^^xsd:nonNegativeInteger
                                  ] .


###  https://w3id.org/sml/def#Mixture
sml:Mixture rdf:type owl:Class ;
            rdfs:subClassOf sml:Matter .


###  https://w3id.org/sml/def#Object
sml:Object rdf:type owl:Class ;
           rdfs:subClassOf sml:Entity ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty sml:hasPart ;
                             owl:allValuesFrom sml:Object
                           ] .


###  https://w3id.org/sml/def#Objectification
sml:Objectification rdf:type owl:Class ;
                    rdfs:subClassOf sml:AbstractConcept ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty rdf:value ;
                                      owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                    ] .


###  https://w3id.org/sml/def#PhysicalObject
sml:PhysicalObject rdf:type owl:Class ;
                   rdfs:subClassOf sml:Object .


###  https://w3id.org/sml/def#PlannedEntity
sml:PlannedEntity rdf:type owl:Class ;
                  rdfs:subClassOf sml:Entity ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty sml:hasPart ;
                                    owl:allValuesFrom sml:PlannedEntity
                                  ] ;
                  owl:disjointWith sml:RealizedEntity .


###  https://w3id.org/sml/def#Port
sml:Port rdf:type owl:Class ;
         rdfs:subClassOf sml:PhysicalObject .


###  https://w3id.org/sml/def#PureSubstance
sml:PureSubstance rdf:type owl:Class ;
                  rdfs:subClassOf sml:Matter ;
                  rdfs:seeAlso sml-term:PureSubstance ;
                  skos:definition "Chemical matter that has an equal chemical composition and recognizable uniform and isotropic properties"@en ;
                  skos:prefLabel "Pure substance"@en .


###  https://w3id.org/sml/def#QualityValue
sml:QualityValue rdf:type owl:Class ;
                 rdfs:subClassOf sml:Objectification ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty rdf:value ;
                                   owl:allValuesFrom [ rdf:type owl:Class ;
                                                       owl:unionOf ( xsd:anyURI
                                                                     xsd:boolean
                                                                     xsd:string
                                                                   )
                                                     ]
                                 ] .


###  https://w3id.org/sml/def#QuantityValue
sml:QuantityValue rdf:type owl:Class ;
                  rdfs:subClassOf sml:Objectification ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty rdf:value ;
                                    owl:allValuesFrom [ rdf:type owl:Class ;
                                                        owl:unionOf ( xsd:decimal
                                                                      xsd:double
                                                                      xsd:float
                                                                      xsd:integer
                                                                    )
                                                      ]
                                  ] .


###  https://w3id.org/sml/def#RealObject
sml:RealObject rdf:type owl:Class ;
               rdfs:subClassOf sml:PhysicalObject ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty sml:hasPart ;
                                 owl:allValuesFrom [ rdf:type owl:Class ;
                                                     owl:unionOf ( sml:Port
                                                                   sml:RealObject
                                                                 )
                                                   ]
                               ] ;
               owl:disjointWith sml:SpatialRegion .


###  https://w3id.org/sml/def#RealizedEntity
sml:RealizedEntity rdf:type owl:Class ;
                   rdfs:subClassOf sml:Entity ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty sml:hasPart ;
                                     owl:allValuesFrom sml:RealizedEntity
                                   ] .


###  https://w3id.org/sml/def#RelationReference
sml:RelationReference rdf:type owl:Class ;
                      rdfs:subClassOf sml:Objectification ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty rdf:value ;
                                        owl:allValuesFrom xsd:anyURI
                                      ] .


###  https://w3id.org/sml/def#Representation
sml:Representation rdf:type owl:Class ;
                   rdfs:subClassOf sml:InformationObject ;
                   rdfs:seeAlso sml-term:Representation ;
                   skos:definition "That which represents something else"@en ;
                   skos:prefLabel "Representation"@en .


###  https://w3id.org/sml/def#Requirement
sml:Requirement rdf:type owl:Class ;
                rdfs:subClassOf sml:InformationObject ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty rdf:value ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
