                                                           owl:unionOf ( EnergyResourceOntology:BuildingEnvironment
                                                                         EnergyResourceOntology:Controllable
                                                                       )
                                                         ] ;
                                             rdfs:range EnergyResourceOntology:FieldOfApplication ;
                                             rdfs:comment "The field of application of a certain facility or zone"@en ;
                                             rdfs:label "hasFieldOfApplication" .


